import java.util.Scanner;
public class Main{
    public static void main(String args[])
    {
        int n;
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter number");
        n=sc.nextInt();
        int fact=1;
        while(n>0)
        {
            fact=fact*n;
            n=n-1;
        }
        System.out.println("Factorial of a number is "+fact);
    }
}
